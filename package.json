{
  "name" : "willbe",
  "version" : "0.5.129",
  "description" : "Utility to manage modules of complex modular systems",
  "author" : "Kostiantyn Wandalen <wanalen@willbe.so>",
  "contributors" :
  [
    { "name" : "Kostiantyn Wandalen", "email" : "wanalen@willbe.so" },
    { "name" : "Volodymyr M.", "email" : "volodymyr.migdal@gmail.com" },
    { "name" : "Dmytro Kryvoruchko", "email" : "dm.vict.kr@gmail.com" }
  ],
  "license" : "MIT",
  "enabled" : 1,
  "main" : "proto/dwtools/atop/will/MainTop.s",
  "files" : [ "proto/dwtools/atop/will", "proto/dwtools/Tools.s", "out" ],
  "bin" :
  {
    "single-process-will" : "proto/dwtools/atop/will/Exec",
    "debug-will" : "proto/dwtools/atop/will/ExecDebug",
    "unrestricted-will" : "proto/dwtools/atop/will/ExecUnrestricted",
    "will" : "proto/dwtools/atop/will/ExecUnrestricted"
  },
  "scripts" :
  {
    "test" : "wtest .run proto/**",
    "docgen" : "wdocgen .build proto",
    "single-process-will" : "node proto/dwtools/atop/will/Exec",
    "debug-will" : "node proto/dwtools/atop/will/ExecDebug",
    "unrestricted-will" : "node proto/dwtools/atop/will/ExecUnrestricted",
    "will" : "node proto/dwtools/atop/will/ExecUnrestricted",
    "docker-build-centos" : "docker image build -t willbe-centos -f ./docker/centos.dockerfile .",
    "docker-build-ubuntu" : "docker image build -t willbe-ubuntu -f ./docker/ubuntu.dockerfile .",
    "docker-run-centos" : "docker run --rm willbe-centos",
    "docker-run-ubuntu" : "docker run --rm willbe-ubuntu"
  },
  "repository" : { "type" : "git", "url" : "https://github.com/Wandalen/willbe.git" },
  "bugs" :
  {
    "url" : "https://github.com/Wandalen/willbe/issues"
  },
  "dependencies" :
  {
    "wTools" : "beta",
    "wCopyable" : "beta",
    "wLogger" : "beta",
    "wverbal" : "beta",
    "wtemplatetreeenvironment" : "beta",
    "wtemplatefilewriter" : "beta",
    "wappbasic" : "beta",
    "wgraphbasic" : "beta",
    "wFiles" : "beta",
    "wfilesarchive" : "beta",
    "wfilesencoders" : "beta",
    "wstatestorage" : "beta",
    "wstatesession" : "beta",
    "wstager" : "beta",
    "wgraphbase" : "beta",
    "wselector" : "beta",
    "wresolver" : "beta",
    "wresolverextra" : "beta",
    "wcommandsaggregator" : "beta",
    "wcommandsconfig" : "beta",
    "wnamemapper" : "beta",
    "wtranspilationstrategy" : "beta",
    "wnpmtools" : "beta",
    "wyamltools" : "beta",
    "tar" : "6.0.1",
    "gitconfiglocal" : "2.1.0",
    "open" : "7.0.3",
    "getos" : "3.2.0"
  },
  "devDependencies" : { "wTesting" : "beta" },
  "keywords" : [ "wTools" ]
}